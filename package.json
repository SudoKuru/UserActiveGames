  "dependencies": {
    "@types/mongoose": "^5.11.97",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.3.0",
    "express-validator": "^6.14.2",
    "mongoose": "^6.8.1",
    "serverless-http": "^3.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
